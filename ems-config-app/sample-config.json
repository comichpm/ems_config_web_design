  "algorithmConfig": {
    "peakShaving": {
      "enabled": true,
      "peakPeriods": [
        { "name": "早高峰", "startTime": "08:00", "endTime": "11:00", "action": "discharge", "maxPower": 500 },
        { "name": "午高峰", "startTime": "14:00", "endTime": "17:00", "action": "discharge", "maxPower": 500 },
        { "name": "晚高峰", "startTime": "19:00", "endTime": "22:00", "action": "discharge", "maxPower": 500 }
      ],
      "valleyPeriods": [
        { "name": "谷电时段", "startTime": "23:00", "endTime": "07:00", "action": "charge", "maxPower": 300 }
      ]
    },
    "demandControl": {
      "enabled": true,
      "demandLimit": 1000,
      "warningThreshold": 85,
      "actionThreshold": 95,
      "responseTime": 30,
      "controlMode": "auto"
    },
    "socManagement": {
      "minSoc": 10,
      "maxSoc": 95,
      "targetSoc": 50,
      "emergencyReserve": 20
    },
    "powerControl": {
      "rampRate": 100,
      "maxChargePower": 500,
      "maxDischargePower": 500,
      "powerFactor": 0.95
    }
  },
  "northboundConfig": {
    "enabled": true,
    "protocol": "mqtt",
    "serverIp": "ems-cloud.example.com",
    "serverPort": 1883,
    "topic": "ems/industrial_park/data",
    "username": "ems_client",
    "password": "",
    "publishInterval": 5000
  },
  "alarmRules": [
    {
      "name": "PCS过压告警",
      "condition": "pcs_voltage > threshold",
      "threshold": 420,
      "unit": "V",
      "level": "warning",
      "notifications": ["popup", "sound"]
    },
    {
      "name": "BMS SOC过低",
      "condition": "soc < threshold",
      "threshold": 10,
      "unit": "%",
      "level": "error",
      "notifications": ["popup", "sound", "sms"]
    },
    {
      "name": "通信中断告警",
      "condition": "offline_time > threshold",
      "threshold": 30,
      "unit": "s",
      "level": "critical",
      "notifications": ["popup", "sound", "email", "sms"]
    }
  ],
  "virtualPoints": [
    {
      "name": "系统总功率",
      "formula": "SUM(pcs_power)",
      "unit": "kW",
      "description": "所有PCS功率之和"
    },
    {
      "name": "系统SOC",
      "formula": "AVG(bms_soc)",
      "unit": "%",
      "description": "所有BMS平均SOC"
    }
  ],
